{
  "eagle": {
    "api_url": "{{EAGLE_API_URL}}",
    "timeout": 30.0,
    "default_item_limit": 50,
    "max_item_limit": 500,
    "default_folder_limit": 100,
    "max_folder_limit": 1000
  },
  "mcp": {
    "server_name": "Eagle MCP Server v2",
    "server_version": "0.1.0",
    "server_description": "A modern MCP server for Eagle App",
    "config_path": "{{MCP_CONFIG_PATH}}",
    "log_level": "INFO",
    "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "paths": {
    "user_data_dir": "{{USER_DATA_DIR}}",
    "cache_dir": "{{CACHE_DIR}}",
    "log_dir": "{{LOG_DIR}}"
  },
  "claude_desktop": {
    "config_path": "{{CLAUDE_DESKTOP_CONFIG_PATH}}"
  },
  "lm_studio": {
    "config_path": "{{LM_STUDIO_CONFIG_PATH}}",
    "conversations_dir": "{{LM_STUDIO_CONVERSATIONS_DIR}}"
  }
}